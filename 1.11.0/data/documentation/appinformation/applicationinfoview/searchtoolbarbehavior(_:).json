        },
        {
          "inlineContent" : [
            {
              "text" : "This modifier can be used to change the default behavior of a search",
              "type" : "text"
            },
            {
              "text" : " ",
              "type" : "text"
            },
            {
              "text" : "field that appears in the toolbar. Place this modifier after the",
              "type" : "text"
            },
            {
              "text" : " ",
              "type" : "text"
            },
            {
              "code" : "View\/searchable(text:isPresented:placement:prompt:)",
              "type" : "codeVoice"
            },
            {
              "text" : " modifier that",
              "type" : "text"
            },
            {
              "text" : " ",
              "type" : "text"
            },
            {
              "text" : "renders search in the toolbar.",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
        {
          "inlineContent" : [
            {
              "text" : "On iPhone, the search field in the bottom toolbar can be configured to",
              "type" : "text"
            },
            {
              "text" : " ",
              "type" : "text"
            },
            {
              "text" : "appear as a button-like control when inactive:",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
        {
          "code" : [
            "@State private var searchText = \"\"",
            "",
            "NavigationStack {",
            "    RecipeList()",
            "        .searchable($searchText)",
            "        .searchToolbarBehavior(.minimized)",
            "}"
          ],
          "syntax" : null,
          "type" : "codeListing"
        }
      ],
