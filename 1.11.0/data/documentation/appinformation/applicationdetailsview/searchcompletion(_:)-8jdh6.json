        },
        {
          "inlineContent" : [
            {
              "text" : "Use this method to associate a search token with a view that is",
              "type" : "text"
            },
            {
              "text" : " ",
              "type" : "text"
            },
            {
              "text" : "within a search suggestion list context. The system uses this value",
              "type" : "text"
            },
            {
              "text" : " ",
              "type" : "text"
            },
            {
              "text" : "when the view is selected to replace the partial text being currently",
              "type" : "text"
            },
            {
              "text" : " ",
              "type" : "text"
            },
            {
              "text" : "edited of the associated search field.",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
        {
          "code" : [
            "enum FruitToken: Hashable, Identifiable, CaseIterable {",
            "    case apple",
            "    case pear",
            "    case banana",
            "",
            "    var id: Self { self }",
            "}",
            "",
            "@State private var text = \"\"",
            "@State private var tokens: [FruitToken] = []",
            "",
            "SearchPlaceholderView()",
            "    .searchable(text: $text, tokens: $tokens) { token in",
            "        switch token {",
            "        case .apple: Text(\"Apple\")",
            "        case .pear: Text(\"Pear\")",
            "        case .banana: Text(\"Banana\")",
            "        }",
            "    }",
            "    .searchSuggestions {",
            "        Text(\"üçé\").searchCompletion(FruitToken.apple)",
            "        Text(\"üçê\").searchCompletion(FruitToken.pear)",
            "        Text(\"üçå\").searchCompletion(FruitToken.banana)",
            "    }"
          ],
          "syntax" : null,
          "type" : "codeListing"
        }
      ],
