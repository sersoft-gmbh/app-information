          "inlineContent" : [
            {
              "text" : "Use this modifier to override the default slider thumb",
              "type" : "text"
            },
            {
              "text" : " ",
              "type" : "text"
            },
            {
              "text" : "visibility. For example, the code below creates a ",
              "type" : "text"
            },
            {
              "code" : "Slider",
              "type" : "codeVoice"
            },
            {
              "text" : " without an",
              "type" : "text"
            },
            {
              "text" : " ",
              "type" : "text"
            },
            {
              "text" : "indicator:",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
        {
          "code" : [
            "@State private var speed = 50.0",
            "@State private var isEditing = false",
            "",
            "var body: some View {",
            "    VStack {",
            "        Slider(",
            "            value: $speed,",
            "            in: 0...100,",
            "            onEditingChanged: { editing in",
            "                isEditing = editing",
            "            }",
            "        )",
            "        .sliderThumbVisibility(.hidden)",
            "",
            "        Text(\"\\(speed)\")",
            "            .foregroundColor(isEditing ? .red : .blue)",
            "    }",
            "}"
          ],
          "syntax" : null,
          "type" : "codeListing"
        },
        {
          "inlineContent" : [
            {
              "text" : "Note: On watchOS, the slider thumb is always visible.",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        }
