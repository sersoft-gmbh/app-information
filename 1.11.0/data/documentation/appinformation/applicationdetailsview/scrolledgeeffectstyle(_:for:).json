        {
          "inlineContent" : [
            {
              "text" : "By default, a scroll view renders an automatic edge effect.",
              "type" : "text"
            },
            {
              "text" : " ",
              "type" : "text"
            },
            {
              "text" : "Use this modifier to change the scroll edge effect style.",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
        {
          "code" : [
            "ScrollView {",
            "    LazyVStack {",
            "        ForEach(data) { item in",
            "            RowView(item)",
            "        }",
            "    }",
            "}",
            ".scrollEdgeEffectStyle(.hard, for: .all)"
          ],
          "syntax" : null,
          "type" : "codeListing"
        }
